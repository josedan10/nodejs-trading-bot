{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "google",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "require-jsdoc":"error",
        "prettier/prettier": ["error", {}, {
            "usePrettierrc": true
        }],
        "semi": "off"
    },
    "root": true,
    "reportUnusedDisableDirectives": true
}
